    "import sys  \n",
    "from PyQt5 import QtWidgets  \n",
    "#pyqt窗口必须在QApplication方法中使用  \n",
    "app=QtWidgets.QApplication(sys.argv)  \n",
    "label=QtWidgets.QLabel(\"<p style='color: red; margin-left: 20px'><b>hell world</b></p>\")#qt支持html标签，强大吧  \n",
    "#有了实例，就需要用show()让他显示  \n",
    "label.show()\n",
    "sys.exit(app.exec_())"
   ]
